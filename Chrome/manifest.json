{
	"name": "Facebook Color Changer",
	"version": "0.9.80",
	"manifest_version": 2,
	"description": "Change the Color of your Facebook!",
	"icons": {
		"16":"icon16.png",
		"48":"icon48.png",
		"128":"icon128.png"
	},
	"permissions": [
  		"tabs", "http://*/*", "https://*/*"
  	],
	"browser_action": {
		"default_title": "Facebook Color Changer",
    	"default_icon": "icon.png",
    	"default_popup": "popup.html"
	}
}
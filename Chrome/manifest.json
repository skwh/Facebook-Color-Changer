{
	"name": "Facebook Color Changer",
	"version": "0.9.9",
	"manifest_version": 2,
	"description": "Change the Color of your Facebook!",
	"permissions": [
  		"tabs", "http://*/*", "https://*/*"
  	],
	"browser_action": {
		"default_title": "Facebook Color Changer",
    	"default_icon": "icon.png",
    	"default_popup": "popup.html"
	}
}